<!DOCTYPE html>
<html>
<head>
  <title>TB</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
    <nav class="navbar navbar-inverse navbar-fixed-top">
      <div class="container">
        <%= link_to "LOGO", '#', id: "logo" %>
            <ul class="nav navbar-nav pull-right">
              <li class='text-primary'><%= link_to "Home",    '#' %></li>
              <li><%= link_to "Help",    '#' %></li>
              <li><%= link_to 'Profile', edit_user_registration_path %></li>
              <% if user_signed_in? %>
                <li><%= link_to "Logout", destroy_user_session_path, method: :delete %></li>
              <% else %>
                <li><%= link_to "SingUp", new_user_registration_path %></li>
                <li><%= link_to "Login", new_user_session_path %></li>
              <% end %>
            </ul>
        </div>
    </nav>
    <div class="row">
      <ul class="nav nav-tabs">
        <li role="presentation" class="active col-lg-3 center"><%= link_to 'Bundle Books', new_book_path %></li>
        <li role="presentation" class="col-lg-3 center"><%= link_to 'Buy Books', books_path %></li>
        <li role="presentation" class="col-lg-3 center"><%= link_to 'Sell Books', new_book_path %></li>
        <li role="presentation" class="col-lg-3 center"><%= link_to 'Quick Look', lookup_quick_look_path %></li>
      </ul>
    </div>
<%#    <p class="notice"><%= notice %><%# </p>
<%#     <p class="alert"><%= alert %>
    <div class="container">
      <%= yield %>
    </div>
</body>
</html>
